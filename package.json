{
  "name": "@nntruong02061999/cloudflare-scraper",
  "version": "1.0.2",
  "publishConfig": {
    "access": "public"
  },
  "description": "Your library description",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "index.d.ts"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "babel src -d dist/cjs --env-name cjs",
    "build:esm": "babel src -d dist/esm --env-name esm",
    "prepare": "npm run build"
  },
  "keywords": [
    "cloudflare",
    "scraper",
    "bypass",
    "chrome",
    "request",
    "anti-bot"
  ],
  "license": "MIT",
  "dependencies": {
    "tough-cookie": "^4.1.3",
    "got": "^11.8.6",
    "chrome-launcher": "^1.0.0",
    "chromium": "^3.0.3",
    "chrome-remote-interface": "^0.33.0",
    "xvfb": "^0.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9"
  },
  "peerDependencies": {
    "node": ">=14.0.0"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "none",
    "parser": "flow",
    "semi": true,
    "tabWidth": 2
  },
  "types": "index.d.ts",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/nntruong02069999/cloudflare-scraper"
  },
  "bugs": {
    "url": "https://github.com/nntruong02069999/cloudflare-scraper/issues"
  },
  "homepage": "https://github.com/nntruong02069999/cloudflare-scraper#readme"
}